<Application x:Class="UST_Routing.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UST_Routing">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary>
					<ResourceDictionary.MergedDictionaries>
						<ResourceDictionary Source="pack://application:,,,/Material;component/Themes/Material.xaml"/>
						<ResourceDictionary Source="pack://application:,,,/UST Routing;component/Themes/Material.xaml"/>
					</ResourceDictionary.MergedDictionaries>
					<local:AppBootstrapper x:Key="bootstrapper" />
				</ResourceDictionary>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
